<h1>Countries#index</h1>
<p>Find me in app/views/countries/index.html.erb</p>

<h2>Countries</h2>
<%= image_tag 'flags.jpg', class: 'flags' %><br />
<%= link_to 'Add new country', new_country_path %><br /><br />
<% @countries.each do |country| %>
  <h2>
    <%= country.name + " " %>
    <%= link_to '[See cheeses]', country_cheeses_path(country) %>
  </h2>
<% end %>
